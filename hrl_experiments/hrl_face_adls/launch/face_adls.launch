<launch>
    <node pkg="hrl_face_adls" name="face_adls_manager" type="face_adls_manager.py"
          output="screen" >
        <remap from="/face_adls/feedback" to="/wt_log_out" />
    </node>
    <node pkg="hrl_head_tracking" name="head_tracking_srv" type="head_tracking_srv"
          output="screen" args="$(find hrl_head_tracking)/data/kelsey_2_face.bag">
          <param name="max_iters" value="300" />
          <param name="color_weight" value="0.0" />
          <param name="icp_max_corresp" value="0.3" />
          <param name="icp_trans_eps" value="0.0000000001" />
    </node>
    <node pkg="hrl_face_adls" name="registration_loader" type="registration_loader.py"
          output="screen" args="$(find hrl_ellipsoidal_control)/data/$(arg subject)_ellipsoid_registration.bag" />
    <node pkg="hrl_netft" name="netft_gravity_zeroing" type="netft_zeroing.py" 
          args="-l -r -z -p -f $(find hrl_face_adls)/params/ft_zero_params_razor_inline.yaml" 
          output="screen" />
    <!--
    <include file="$(find hrl_rfh_fall_2011)/launch/head_registration.launch" />
    <include file="$(find kelsey_sandbox)/launch/pr2_ar_servo.launch" />
    <node pkg="hrl_rfh_fall_2011" name="setup_arms_shaving" type="setup_arms_shaving.py"
          output="screen" args="-s" />
    <node pkg="hrl_rfh_fall_2011" name="setup_arms_servoing" type="setup_arms_servoing.py"
          output="screen" args="-s" />
    <node pkg="hrl_rfh_fall_2011" name="setup_registration" type="setup_registration.py"
          output="screen" args="-s" />
    <node pkg="pr2_traj_playback" name="traj_playback_r" type="arm_pose_move_controller.py"
          output="screen" args="-v -r" />
    <node pkg="pr2_traj_playback" name="traj_playback_l" type="arm_pose_move_controller.py"
          output="screen" args="-v -l" />
    -->

</launch>
