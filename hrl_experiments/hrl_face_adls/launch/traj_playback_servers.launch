<launch>
    
    <arg name="subject" />
    <node pkg="pr2_traj_playback" name="l_arm_pose_move_controller" type="arm_pose_move_controller.py"
          output="screen" args="-v -l" >
        <remap from="/trajectory_playback_l/feedback" to="/wt_log_out" />
    </node>
    <node pkg="pr2_traj_playback" name="r_arm_pose_move_controller" type="arm_pose_move_controller.py"
          output="screen" args="-v -r" >
        <remap from="/trajectory_playback_r/feedback" to="/wt_log_out" />
    </node>

    <rosparam>
        face_adls_traj_files:
            "[L] Move to side for servo approach" : "$(find hrl_face_adls)/data/l_arm_servo_setup.pkl"
            "[R] Move to side for servo approach" : "$(find hrl_face_adls)/data/r_arm_servo_setup.pkl"
            "[L] Left cheek - Untuck arm to face" : "$(find hrl_face_adls)/data/l_arm_shaving_setup_l.pkl"
            "[R] Left cheek - Untuck arm to face" : "$(find hrl_face_adls)/data/r_arm_shaving_setup_l.pkl"
            "[L] Right cheek - Untuck arm to face" : "$(find hrl_face_adls)/data/l_arm_shaving_setup_r.pkl"
            "[R] Right cheek - Untuck mirror to view" : "$(find hrl_face_adls)/data/r_arm_mirror_setup_r.pkl"
    </rosparam>
</launch>
